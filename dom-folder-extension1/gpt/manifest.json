{
  "manifest_version": 3,
  "name": "DOM Folder",
  "version": "0.1",
  "description": "Экспериментальное расширение для сворачивания DOM-элементов.",
  "action": {
    "default_popup": "popup.html",
    "default_title": "DOM Tools"
  },
  "permissions": [
    "scripting",
    "activeTab"
  ],
  "host_permissions": [
    "<all_urls>"
  ]
}
